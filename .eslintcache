st haze = useScrub(hazeKfs, relY)\n\n\n  return (\n    <motion.div\n      id='tile-parent'\n      style={{\n        height:tileHeight,\n        width:tileWidth,\n        top:tileTop,\n        left:tileLeft,\n        opacity:tileOpacity,\n      }}\n      sx={{\n        position:'absolute',\n        flexDirection:'column',\n        justifyContent:'center',\n        alignItems:'center',\n        display:relY >= .23 ? 'flex' : 'none'\n      }}>\n      <motion.div\n        id='tile-glow'\n        style={{\n          backgroundImage:radialGradient,\n          height:glowSize,\n          width:glowSize,\n          transform:spin\n        }}\n        sx={{\n          mixBlendMode:'soft-light',\n          position:'absolute',\n          zIndex:10,\n        }}>\n      </motion.div>\n      <motion.div\n        id='tile'\n        style={{\n          borderRadius:tileRadius,\n          transform:spin,\n        }}\n        sx={{\n          height:'100%',\n          width:'100%',\n          mixBlendMode:'normal',\n          zIndex:104,\n          overflow:'hidden',\n          bg:'light'\n        }}>\n        <Content type={type}/>\n        <a href={content[type].tiles[id].url} sx={{height:'fit-content',display:'block'}}>\n          <motion.div\n            id='cursor-melt'\n            style={{opacity:haze}}\n            sx={{\n              cursor:'pointer',\n              height:relY > .36 && relY < .82 ? '95%' : '100%',\n              width:relY > .36 && relY < .82 ? '95%' : '100%',\n              m:relY > .36 && relY < .82 ? '2.5%' : 0,\n              position:'absolute',\n              top:0,\n              left:0,\n              bg:'light'\n            }}>\n          </motion.div>\n        </a>\n      </motion.div>\n    </motion.div>\n  )\n}\n","/Users/lauren/Desktop/repos/ld-home/src/views/landscape/Content.js",[],"/Users/lauren/Desktop/repos/ld-home/src/hooks/use-bounding-box.js",[],{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","se